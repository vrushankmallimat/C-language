//To identify upper and lower case alphabet
#include<stdio.h>
int main()
{
  char ch;

  printf("Enter the character: ");
  scanf( "%c", &ch);

  if(ch>='A' && ch<='Z')
  {
  printf("The given character %c is Upper case", ch);
  }

  else if(ch>='a' && ch<='z')
  {
printf("The given character %c is lower case", ch);
  }

else{
  printf("The input given is not an english alphabet");
}
  return 0;
}
